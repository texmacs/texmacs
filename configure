# compare dotted numbers
# return true if the first is biggest
function cmp_dot_number {
  local -i n1 n2 i=0
  n1=(${1//./ })
  n2=(${2//./ })
  while test -n "${n1[$i]}" -a -n "${n2[$i]}"
  do ((${n1[$i]} > ${n2[$i]}))  && return 0
     ((${n1[$i]} < ${n2[$i]}))  && return 1
     i+=1
  done
  test -n ${n1[$i]} && return 0
  return 1
}


