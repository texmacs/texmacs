        CFLAGS="${CFLAGS/%[[:space:]]$STRIP_ALL_ARGS_arg/}"    # at the end
        CFLAGS="${CFLAGS//[[:space:]]$STRIP_ALL_ARGS_arg[[:space:]]/ }"  # all in the middle no consecutive
      done
    fi
  CFLAGS=$(echo $CFLAGS)  # wipe extra blank if any
  unset ${!STRIP_ARG_*}

