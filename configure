  fi

  #--- Warnings

  case "$GXX_VERSION" in
    none)
      CONFIG_CXXWARNING="";
    ;;
    *)
      CONFIG_CXXWARNING="-Wall -Wno-return-type"
    ;;
  esac

  # Check whether --enable-warnings was given.
if test "${enable_warnings+set}" = set; then :
  enableval=$enable_warnings;
else
  enable_warnings="yes"
